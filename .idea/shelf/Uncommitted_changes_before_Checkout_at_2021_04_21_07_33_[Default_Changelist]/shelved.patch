+public class Corona extends Location {
+    private static Integer numRelevantReports;
+    public static final Integer categoryId = 2;
+    private static Seriousness seriousness;
+
+    @Override
+    public void updateScaleSeriousness() {
+        if(numRelevantReports < 3){
+            seriousness = Seriousness.NOTSERIOUS;
+        }else if(numRelevantReports == 3){
+            seriousness = Seriousness.KINDA;
+        }else if(numRelevantReports < 10) {
